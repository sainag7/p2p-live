# Copy to .env.local and set your key. Never commit .env.local or any file containing real keys.
# Used only by the ops API server (server/index.js) for the Complaints LLM summary feature.

GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Gemini model (default: gemini-2.0-flash)
# GEMINI_MODEL=gemini-2.0-flash

# Optional: Ops API server port (default: 3000)
# OPS_API_PORT=3000


# Mapbox: client (tiles) and server (Directions API only)
VITE_MAPBOX_TOKEN=your_mapbox_token_here
# Server-only; never expose. Used for /api/mapbox/route (Directions).
MAPBOX_TOKEN=your_mapbox_token_here

VITE_API_BASE_URL = https://p2p-live.onrender.com

# Optional: base URL for ops API (route proxy). Default same-origin or http://localhost:3001
# VITE_OPS_API_URL=http://localhost:3001